@model IEnumerable<LexiconLMSPortal.Models.ViewModels._StudentListPartial>
@{
    Layout = "";
}
<div id="StudentListPartial">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th></th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FullName) <i class="glyphicon glyphicon-pencil"></i> <i class="glyphicon glyphicon-remove-circle"></i>
            </td>
        </tr>
    }

    </table>
</div>